<form id="addForm" action="/shopping" method="POST">
  <label for="item">Item:</label>
  <input type="text" name="item" value="" id="item" />
  <label for="qty">Quantity:</label>
  <input type="number" name="qty" value="1" id="qty" />
  <button type="submit"><i class="fas fa-save"></i>Add</button>
</form>
